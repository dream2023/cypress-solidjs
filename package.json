    "cypress": "^10.11.0",
    "solid-js": "^1.6.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.2",
    "vite-plugin-solid": "^2.3.10"
  },
  "peerDependencies": {
    "cypress": ">=10.6.0",
    "solid-js": ">=1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dream2023/cypress-solidjs.git"
  },
  "homepage": "https://github.com/dream2023/cypress-solidjs/#readme"
