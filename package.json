    "check-ts": "tsc --noEmit",
    "np":"npm publish --access public --registry https://registry.npmjs.org"
  },
  "keywords": [
    "cypress",
    "solidjs",
    "solid",
    "testing",
    "component testing"
  ],
  "files": [
